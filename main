class sumsss:
   def __init__(self,name,sums):
      self.name = name
      self.sums = sums
class dt:
   def __init__(self,cost, name):
      self.cost = cost
      self.gets = {}
      self.getones = {}
      self.name = name
   def addget(self,name,pb):
      self.gets[name] = pb
      self.getones[name] = 1.0/pb
      # print(1.0/pb)

   def getone(self,name):
      # print(self.getones[name])
      return self.getones[name]

   def playsum(self, ps, sums=1.0):
      for i in self.gets:
         ps[i] = ps.get(i,0.0) + self.gets[i]*sums

class cl:
   def __init__(self, name):
      self.dtlist = []
      self.name = name
      self.next = {}

   def addk(self, name, sums):
      self.next[name] = sums

   def getnext(self,sums,p):
      p[self.name] = p[self.name] - sums
      if p[self.name] <=0:
         p.pop(self.name)
      for i in self.next:
         p[i] = p.get(i, 0.0) + self.next[i] * sums


def deletes(a,b): # a是need
   for i in a:
      a[i] = a[i] - b.get(i, 0.0)
   p = True
   while p:
      p = False
      for i in a:
         if a[i] <=0:
            a.pop(i)
            p = True
            break
cls = {}
dts = []

sumf = 0
bestsolves = []
mincost = None
lastcost = mincost
equr = False
def solve(needs):
   global bestsolves,mincost,lastcost,sumf
   bestsolves = []
   mincost = None
   solve2(needs, 0, [])
   lastcost = mincost

   # def deep(needs1):
   #    global lastcost
   #    for i1 in needs1:
   #       if len(cls[i1].next):
   #          f1 = needs1.copy()
   #          f1,lastcost = loplist(lastcost, f1, i1)
   #          print(f1)
   #          deep(f1)
   #
   # deep(needs)
   for i in needs:
      if len(cls[i].next):
         sumf += 1
         print(sumf, end='\r')
         f = needs.copy()
         lastcost = loplist(lastcost,f,i)
         for j in f:
            if len(cls[j].next):
               g = f.copy()
               sumf += 1
               print(sumf, end='\r')
               # print(g,1)
               lastcost = loplist(lastcost, g, j)
               for k in g:
                  if len(cls[k].next):
                     t = g.copy()
                     sumf += 1
                     print(sumf, end='\r')
                     # print(t,2)
                     lastcost = loplist(lastcost, t, k)
                     for fs in t:
                        if len(cls[k].next):
                           sumf+=1
                           print(sumf,end='\r')
                           r = t.copy()
                           # print(t, 2)
                           lastcost = loplist(lastcost, r, fs)


def loplist(lastcost1,f,i):
   global  equr
   while f.get(i,0) != 0:

      cls[i].getnext(1, f)
      # print(f)
      equr = False
      solve2(f, 0, [])
      if not equr and lastcost1 is not None:
         # print(lastcost1,1)
         return f,lastcost1
      else:
         lastcost1 = mincost
   # print(f, lastcost, i)
   return lastcost
   #

def solve2(needs, cost, dts1):
   global bestsolves, mincost, sumf, equr
   for i in needs:
      # print(cls[i].dtlist)
      if cls[i].dtlist is not None:
         for j in cls[i].dtlist:
            # print(cost, j.name, needs, mincost)
            sums = int(j.getone(i)*needs[i] + 0.99999)
            if not sums:
               sums+=1
            cost += sums * j.cost
            if mincost and cost > mincost:
               cost = cost - sums * j.cost
               continue
            p1 = (j.name, sums)
            dts1.append(p1)
            p = {}
            j.playsum(p,sums)
            f = needs.copy()
            deletes(f,p)
            # print(cost, j.name, f,mincost)
            if len(f):
               solve2(f, cost, dts1)
            else:
               if mincost == cost:
                  equr = True
               if mincost is None or mincost > cost:
                  equr = True
                  bestsolves = dts1.copy()
                  mincost = cost
            cost = cost - sums * j.cost
            dts1.pop()
def getneed():
   try:
      with open('need.txt','r') as f:
         needs = {}
         f.readline()
         while True:
            a = f.readline()
            b = f.readline()
            if not a or not b:
               return needs
            if int(b[:-1])!= 0:
               needs[a[:-1]] = int(b[:-1])

   except OSError:
