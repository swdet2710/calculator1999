def main():
   times = time.time()
   # needs = {'分辨善恶之果':1}#'盐封曼德拉':5}#,,'分辨善恶之果':2
   needs = getneed()
   print(needs)
   solve(needs)
   print()
   for i in bestsolves:
      print(i[0],' ', i[1], '次')

   p = {}
   for i in bestsolves:
      for j in dts:
         if i[0] == j.name:
            # print(p)
            j.playsum(p,i[1])
            # print(p,j.gets)
   print('消耗',mincost,'点体力，耗时{:2f}s'.format(time.time()-times))
   for i in p:
      print(i,p[i])


if __name__ == '__main__':
   main()
   input('按任意键退出')
